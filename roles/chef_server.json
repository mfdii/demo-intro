{
  "name": "chef_server",
  "description": "Core CM - chef-client.",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "yum" : {
      "update" : false
    },
    "chef_client" : {
        "bin" : "/usr/local/rvm/gems/ruby-1.9.3-p362/bin/chef-client",
        "conf_dir" : "/etc/chef",
        "interval" : "300",
        "server_url" : "https://chef.localdomain/organizations/opscode",
        "validation_client_name" : "opscode-validator"
      }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[chef-client::config]",
    "recipe[yum::epel]"
  ],
  "env_run_lists": {
  }
}
